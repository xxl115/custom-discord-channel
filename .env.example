# Discord Bot Token
# Get from: https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_bot_token_here

# Proxy Configuration
DISCORD_PROXY_ENABLED=true
DISCORD_PROXY_URL=socks5://127.0.0.1:7891

# Gateway Intents
DISCORD_INTENTS=513

# Test Channel ID (optional)
TEST_CHANNEL_ID=your_channel_id_here
